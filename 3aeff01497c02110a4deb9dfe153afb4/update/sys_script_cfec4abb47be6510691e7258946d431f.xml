        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    var CANDIDATE_REQUIREMENT_TABLE = 'x_431105_cand_ass_candidate_requirement';
    var PUBLICATION_REQUIREMENT_TABLE = 'x_431105_cand_ass_competition_requirment';
    var grCandidateRequirement = new GlideRecord(CANDIDATE_REQUIREMENT_TABLE);
    var grPublicationRequirement = new GlideRecord(PUBLICATION_REQUIREMENT_TABLE);
    grPublicationRequirement.addQuery('competition', current.assessment);
    grPublicationRequirement.query();
    while (grPublicationRequirement.next()) {
		grCandidateRequirement.initialize();
		grCandidateRequirement.requirement = grPublicationRequirement.sys_id;
		grCandidateRequirement.candidate = current.sys_id;
		grCandidateRequirement.insert();
	}


})(current, previous);]]></script>
