        <action_name/>
        <active>false</active>
        <client>false</client>
        <client_script_v2><![CDATA[function onClick(g_form) {
    // STATES
    var PRESELECTION_CBT = '5165ad5897442110a4deb9dfe153af6f';
    var PRESELECTION = '7f65a1d497442110a4deb9dfe153af65';
    var SELECTION = '8085299897442110a4deb9dfe153af4c';

    var ASSESSMENT = current.sys_id;
    var candidateAssessmentUtils = new CandidateAssessmentUtils();

    candidateAssessmentUtils.cleanUpRecordsWithoutAssessment();
    // Import Eligibility Exclusion Reasons
    candidateAssessmentUtils.addExclusionReasons(ASSESSMENT);
    // Import CBT
    candidateAssessmentUtils.populateAssessmentSCriteria(PRESELECTION_CBT, ASSESSMENT);
    candidateAssessmentUtils.populateAssessmentQuestions(PRESELECTION_CBT);
    // Import Preselection
    candidateAssessmentUtils.populateAssessmentSCriteria(PRESELECTION, ASSESSMENT);
    candidateAssessmentUtils.populateAssessmentQuestions(PRESELECTION);
    // Import Selection
    candidateAssessmentUtils.populateAssessmentSCriteria(SELECTION, ASSESSMENT);
    candidateAssessmentUtils.populateAssessmentQuestions(SELECTION);

    action.setRedirectURL(current);
}]]></client_script_v2>
