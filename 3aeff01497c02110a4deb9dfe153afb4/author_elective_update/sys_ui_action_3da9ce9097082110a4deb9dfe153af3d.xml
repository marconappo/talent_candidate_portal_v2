        <name>NOImport Assessments [SAP/Prom./Hudson]</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[var caUtils = new CandidateAssessmentUtils();
caUtils.cleanUpTables();
caUtils.cleanUpRecordsWithoutAssessment();
caUtils.cleanUpRecordsWithoutCandidate();
var assessmentList = caUtils.importAssessmentsFromSAPServerSide();
var length = assessmentList.length;
for (var i = 0; i < length; i++) {
    var assessment = assessmentList[i];
    caUtils.addExclusionReasons(assessment);
    var stateList = [caUtils.PRESELECTION_CBT, caUtils.SELECTION];
    for (var j = 0; j < stateList.length; j++) {
        caUtils.populateAssessmentSCriteria(stateList[j], assessment);
        caUtils.populateAssessmentQuestions(stateList[j]);
    }
}]]></script>
