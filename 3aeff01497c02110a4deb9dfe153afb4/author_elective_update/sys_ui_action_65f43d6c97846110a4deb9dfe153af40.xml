        <client_script_v2><![CDATA[function onClick(g_form) {
    var CANDIDATE = current.sys_id;
    var ASSESSMENT_STATE = current.assessment.state;
    // ASSESSMENT STATES
    var PRESELECTION_CBT = 3; //'5165ad5897442110a4deb9dfe153af6f';
    var PRESELECTION = 4; //'7f65a1d497442110a4deb9dfe153af65';
    var SELECTION = 5; //'8085299897442110a4deb9dfe153af4c';
    // TABLES
    var ASSESSMENT_SC = 'x_431105_cand_ass_assessment_selection_criteria';
    var CANDIDATE_SC = 'x_431105_cand_ass_candidate_selection_criteria_score';
    var ASSESSMENT_QUESTION = 'x_431105_cand_ass_assessment_question';
    var CANDIDATE_ANSWER = 'x_431105_cand_ass_candidate_answers';

    calculateCandidateScore();
}

function calculateCandidateScore() {
    if (ASSESSMENT_STATE == PRESELECTION_CBT)
        current.preselection_score = calculatePreSelectionScore();
    else if (ASSESSMENT_STATE == PRESELECTION)
        current.preselection_score = calculatePreSelectionScore();
    else if (ASSESSMENT_STATE == SELECTION)
        current.selection_score = calculatePreSelectionScore();
    current.update();
}

function calculatePreSelectionScore() {
    var gr = new GlideRecord(CANDIDATE_SC);
    gr.addQuery('candidate', CANDIDATE);
    gr.addQuery('assessment_selection_criteria.parent', '');
    gr.addQuery('assessment_selection_criteria.assessment_state', getStateSysID(ASSESSMENT_STATE));
    gr.query();
    var score = 0;
    while (gr.next()) score = score + gr.score;
    //gs.addInfoMessage('Score: ' + score);
    return score;
}

function getStateSysID(state) {
    if (state == '3') return '5165ad5897442110a4deb9dfe153af6f';
    if (state == '4') return '7f65a1d497442110a4deb9dfe153af65';
    if (state == '5') return '8085299897442110a4deb9dfe153af4c';
    return state;
}]]></client_script_v2>
