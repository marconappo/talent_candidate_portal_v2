        <client_script_v2><![CDATA[function onClick(g_form) {
    var CANDIDATE = current.candidate;
    // ASSESSMENT STATES
    var PRESELECTION_CBT = 3; //'5165ad5897442110a4deb9dfe153af6f';
    var PRESELECTION = 4; //'7f65a1d497442110a4deb9dfe153af65';
    var SELECTION = 5; //'8085299897442110a4deb9dfe153af4c';
    // TABLES
    var ASSESSMENT_SC = 'x_431105_cand_ass_assessment_selection_criteria';
    var CANDIDATE_SC = 'x_431105_cand_ass_candidate_selection_criteria_score';
    var ASSESSMENT_QUESTION = 'x_431105_cand_ass_assessment_question';
    var CANDIDATE_ANSWER = 'x_431105_cand_ass_candidate_answers';
    var MAIN_SELECTION_CRITERIA = current.assessment_selection_criteria;
    var ASSESSMENT_STATE = current.candidate.assessment.state;
    current.score = calculateScore();
    current.update();
    //action.setRedirectURL(current);
}

function calculateScore() {
    var score = 0;
    if (ASSESSMENT_STATE == PRESELECTION || ASSESSMENT_STATE == SELECTION) {
        var gr = new GlideRecord(CANDIDATE_SC);
        gr.addQuery('candidate', CANDIDATE);
        //assessment_selection_criteria.parent=39342e6097886110a4deb9dfe153afbd
        gr.addQuery('assessment_selection_criteria.parent', MAIN_SELECTION_CRITERIA);
        gr.query();
    } else if (ASSESSMENT_STATE == PRESELECTION_CBT) {
        gr = new GlideRecord(CANDIDATE_ANSWER);
        gr.addQuery('candidate', CANDIDATE);
        gr.addQuery('assessment_selection_criteria', MAIN_SELECTION_CRITERIA);
        gr.addQuery('assessment_question.active', 'true');
        gr.query();
    }
    while (gr.next()) score = score + gr.score;
    return score;
}]]></client_script_v2>
